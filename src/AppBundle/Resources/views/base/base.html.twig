<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}L2W!{% endblock %}</title>
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
    {% stylesheets
    'bundles/app/vendor/bootstrap/dist/css/bootstrap.css'
    'bundles/app/vendor/font-awesome/css/font-awesome.css'
    filter='cssrewrite' %}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}
    {% javascripts
    '@AppBundle/Resources/public/vendor/jquery/dist/jquery.js'
    '@AppBundle/Resources/public/vendor/bootstrap/dist/js/bootstrap.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    {% block stylesheets %}{% endblock %}
</head>
<body>
{% block body %}{% endblock %}
{% block javascripts %}{% endblock %}
</body>
</html>
